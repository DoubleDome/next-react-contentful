{
  "name": "fe-react-storybook",
  "version": "0.0.1",
  "description": "",
  "main": "./fe-react-storybook.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/MGMResorts/fe-react-storybook"
  },
  "bin": {
    "fe-react-storybook-gen2-build": "./bin/dmp-fe-build.js"
  },
  "scripts": {
    "dev": "concurrently --kill-others \"node server.js\" \"npm run start\"",
    "next": "npm run dmp:build && node server.js",
    "setup": "node bin/setup.js",
    "start": "NODE_OPTIONS=--max_old_space_size=4096 npm run storybook:serve",
    "storybook:serve": "start-storybook -p 3001 -c storybook",
    "storybook:build": "build-storybook -c storybook -o storybook-dist",
    "storybook:deploy": "up",
    "dmp:build": "node ./build/init.js",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "test": "jest --json --outputFile=.jest-test-results.json",
    "test:watch": "yarn run test --watchAll",
    "format": "node_modules/.bin/prettier --config .prettierrc \"{build,src,storybook}/**/*.{css,scss,js}\" --write",
    "format-test": "node_modules/.bin/prettier --config .prettierrc \"{build,src,storybook}/**/*.{css,scss,js}\" -l"
  },
  "husky": {
    "hooks": {
      "pre-commit": "node_modules/.bin/lint-staged"
    }
  },
  "lint-staged": {
    "{build,src,storybook}/**/*.js": [
      "eslint"
    ],
    "{build,src,storybook}/**/*.{css,scss,js}": [
      "prettier --config .prettierrc -l"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.1.6",
    "@storybook/addon-a11y": "3.4.11",
    "@storybook/addon-info": "3.4.11",
    "@storybook/addon-jest": "3.4.11",
    "@storybook/addon-knobs": "3.4.11",
    "@storybook/addon-options": "3.4.11",
    "@storybook/addon-storysource": "3.4.11",
    "@storybook/addon-viewport": "3.4.11",
    "@storybook/addons": "3.4.11",
    "@storybook/react": "3.4.11",
    "add": "2.0.6",
    "apollo-server-express": "^2.2.2",
    "apolloclient": "0.0.2",
    "autoprefixer": "^8.2.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-wrap-in-js": "^1.1.1",
    "bootstrap": "^4.0.0",
    "classnames": "2.2.6",
    "contentful": "^5.1.3",
    "dotenv": "6.0.0",
    "express": "^4.16.3",
    "glob": "^7.1.2",
    "graphql": "^14.0.2",
    "inquirer": "^6.2.0",
    "jquery": "^3.3.1",
    "memoize-one": "4.0.2",
    "mobx": "4.5.0",
    "mobx-react": "5.2.3",
    "next": "^5.1.0",
    "nextjs-dynamic-routes": "^2.2.1",
    "node-sass": "^4.9.0",
    "nprogress": "^0.2.0",
    "nuka-carousel": "github:jtomaszewski/nuka-carousel#cdef463",
    "popper.js": "^1.14.3",
    "postcss-easy-import": "^3.0.0",
    "postcss-loader": "^2.1.3",
    "prop-types": "15.6.2",
    "raw-loader": "^0.5.1",
    "react": "16.4.1",
    "react-apollo": "^2.3.1",
    "react-dom": "16.4.1",
    "react-lazyload": "2.3.0",
    "react-portal": "4.1.5",
    "react-responsive": "5.0.0",
    "react-slick": "0.23.2",
    "react-svg-loader": "^2.1.0",
    "react-test-renderer": "16.6.0",
    "react-transition-group": "2.5.0",
    "sass-loader": "^6.0.7",
    "storybook-addon-docgen": "github:jack-sf/react-storybook-addon-docgen#e54d7cb",
    "uuid": "3.3.2",
    "yarn": "1.12.3"
  },
  "devDependencies": {
    "autoprefixer": "9.2.1",
    "babel-core": "6.26.3",
    "babel-eslint": "9.0.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-decorators-legacy": "1.3.5",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-runtime": "6",
    "chalk": "2.4.1",
    "concurrently": "^4.0.1",
    "css-loader": "0.28.11",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.6.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "3.1.0",
    "eslint-config-react-app": "3.0.0",
    "eslint-loader": "2.1.1",
    "eslint-plugin-flowtype": "2.49.3",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.11.0",
    "eslint-scope": "3.7.1",
    "file-loader": "1.1.11",
    "fs-extra": "7.0.0",
    "glob": "7.1.2",
    "husky": "1.1.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.6.0",
    "lint-staged": "7.3.0",
    "mkdirp": "0.5.1",
    "node-sass": "4.9.2",
    "postcss-loader": "2.1.5",
    "prettier": "1.14.3",
    "prettier-eslint": "8.8.2",
    "rimraf": "2.6.2",
    "sass-loader": "7.0.3",
    "style-loader": "0.21.0",
    "webpack": "3.11.0"
  },
  "jest": {
    "setupFiles": [
      "./jest.setup.js"
    ],
    "moduleNameMapper": {
      "@/(.*)$": "<rootDir>/src/$1",
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    }
  },
  "author": "",
  "license": "ISC"
}
